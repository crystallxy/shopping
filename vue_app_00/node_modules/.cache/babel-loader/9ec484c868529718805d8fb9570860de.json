    var regname = /^[a-z0-9_]{8,12}$/i;\n      var regpwd = /^[a-z0-9]{8,12}$/i; //验证用户名\n\n      if (!regname.test(n)) {\n        Toast(\"用户名格式不正确\");\n        return;\n      } //验证密码\n\n\n      if (!regpwd.test(p)) {\n        Toast(\"密码格式不正确\");\n        return;\n      } //验证密码与确认密码一致\n\n\n      if (p != p2) {\n        Toast(\"两次密码不一致，请修改\");\n        return;\n      } //3:发送ajax请求\n\n\n      var url = \"http://127.0.0.1:3000/register?\";\n      url += \"name=\" + n;\n      url += \"&pwd=\" + p;\n      this.axios.get(url).then(function (result) {\n        //console.log(result.data);\n        Toast(result.data.msg);\n      }); //4:提示\n    }\n  }\n};",{"version":3,"sources":["Register.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4BA,SAAA,KAAA,QAAA,SAAA;AACA,eAAA;AACA,MADA,kBACA;AACA,WAAA;AACA,YAAA,EADA;AACA;AACA,WAAA,EAFA;AAGA,YAAA,EAHA;AAIA,eAAA,KAJA,CAIA;;AAJA,KAAA;AAMA,GARA;AASA,WAAA;AACA,aADA,uBACA;AAAA;;AACA;AACA,UAAA,IAAA,KAAA,IAAA,CAFA,CAGA;;AACA,UAAA,MAAA,kCAAA;AACA,aAAA,WAAA,CAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,kBAAA;AACA,YAAA,OAAA,IAAA,CAAA,IAAA,GAAA,CAAA,EAAA;AACA,gBAAA,OAAA;AACA,gBAAA,OAAA,GAAA,IAAA;AACA,SAHA,MAGA;AACA,gBAAA,UAAA;AACA,gBAAA,OAAA,GAAA,KAAA;AACA;AACA,OARA,EANA,CAeA;AACA,KAjBA;AAkBA,aAlBA,uBAkBA;AACA,UAAA,KAAA,OAAA,KAAA,KAAA,EAAA;AAAA;AACA,cAAA,YAAA;AACA;AACA,OAJA,CAKA;AACA;;;AACA,UAAA,IAAA,KAAA,IAAA;AACA,UAAA,IAAA,KAAA,GAAA;AACA,UAAA,KAAA,KAAA,IAAA,CATA,CAUA;;AACA,UAAA,UAAA,oBAAA;AACA,UAAA,SAAA,mBAAA,CAZA,CAaA;;AACA,UAAA,CAAA,QAAA,IAAA,CAAA,CAAA,CAAA,EAAA;AACA,cAAA,UAAA;AACA;AACA,OAjBA,CAkBA;;;AACA,UAAA,CAAA,OAAA,IAAA,CAAA,CAAA,CAAA,EAAA;AACA,cAAA,SAAA;AACA;AACA,OAtBA,CAuBA;;;AACA,UAAA,KAAA,EAAA,EAAA;AACA,cAAA,aAAA;AACA;AACA,OA3BA,CA4BA;;;AACA,UAAA,MAAA,iCAAA;AACA,aAAA,UAAA,CAAA;AACA,aAAA,UAAA,CAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,kBAAA;AACA;AACA,cAAA,OAAA,IAAA,CAAA,GAAA;AACA,OAHA,EAhCA,CAoCA;AACA;AAvDA;AATA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app-register\">\r\n     \t\t<div class=\"mui-card\">\r\n\t\t\t\t<div class=\"mui-card-header\">\r\n           <h3>用户注册</h3>\r\n        </div>\r\n\t\t\t\t<div class=\"mui-card-content\">\r\n\t\t\t\t\t<div class=\"mui-card-content-inner\">\r\n\t\t\t\t\t\t<form>\r\n              用户名:\r\n              <input type=\"text\" value=\"\"\r\n              placeholder=\"请输入用户名\" v-model=\"name\"\r\n              @blur.prevent=\"checkName\" /> \r\n              密码:\r\n              <input type=\"password\" value=\"\"\r\n              placeholder=\"请输入密码\" v-model=\"pwd\" />\r\n              确认密码:\r\n              <input type=\"password\" value=\"\"\r\n              placeholder=\"请输入确认密码\" v-model=\"pwd2\" />\r\n              <mt-button size=\"large\" @click=\"handleReg\">注册</mt-button>\r\n            </form>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"mui-card-footer\">页脚</div>\r\n\t\t\t</div>\r\n  </div>  \r\n</template>\r\n<script>\r\n  import {Toast} from  \"mint-ui\";\r\n  export default {\r\n    data(){\r\n      return {\r\n        name:\"\",  //准备双向绑定数据\r\n        pwd:\"\",\r\n        pwd2:\"\",\r\n        isSumit:false //是否允许提交\r\n      }\r\n    },\r\n    methods:{\r\n      checkName(){\r\n        //1:获取用户输入用户名\r\n        var n = this.name;\r\n        //2:发送ajax请求\r\n        var url = \"http://127.0.0.1:3000/existsName\";\r\n        url+=\"?name=\"+n;\r\n        this.axios.get(url).then(result=>{\r\n          if(result.data.code > 0){\r\n            Toast(\"欢迎使用!\")\r\n            this.isSumit = true;\r\n          }else{\r\n            Toast(\"该用户名己存在!\")\r\n            this.isSumit = false;\r\n          }\r\n        })\r\n        //3:提示消息\r\n      },\r\n      handleReg(){\r\n        if(this.isSumit === false){ //禁止提交\r\n          Toast(\"用户名己存在，请修改\");\r\n          return;\r\n        }\r\n        //1:获取用户输入用户名 密码 确认密码\r\n        //  双向绑定\r\n        var n = this.name;\r\n        var p = this.pwd;\r\n        var p2 = this.pwd2;\r\n        //2:验证\r\n        var regname = /^[a-z0-9_]{8,12}$/i;\r\n        var regpwd = /^[a-z0-9]{8,12}$/i\r\n        //验证用户名\r\n        if(!regname.test(n)){\r\n           Toast(\"用户名格式不正确\");\r\n           return;\r\n        }\r\n        //验证密码\r\n        if(!regpwd.test(p)){\r\n          Toast(\"密码格式不正确\");\r\n          return;\r\n        }\r\n        //验证密码与确认密码一致\r\n        if(p != p2){\r\n          Toast(\"两次密码不一致，请修改\");\r\n          return;\r\n        }\r\n        //3:发送ajax请求\r\n        var url = \"http://127.0.0.1:3000/register?\";\r\n        url +=\"name=\"+n;\r\n        url +=\"&pwd=\"+p\r\n        this.axios.get(url).then(result=>{\r\n            //console.log(result.data);\r\n            Toast(result.data.msg)\r\n        })\r\n        //4:提示\r\n      }\r\n    }\r\n  }  \r\n</script>\r\n<style>\r\n</style>"],"sourceRoot":"src\\components\\tabbar"}]}